<?xml version="1.0" encoding="utf-8"?>

<LinearLayout android:layout_width="fill_parent" android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:id="@+id/dialog" android:minWidth="400px">
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/labelFiles" android:text="Selected Files :" android:layout_margin="5px"></TextView><ScrollView android:id="@+id/ScrollView01" android:layout_width="fill_parent" android:layout_margin="5px" android:layout_height="wrap_content"><TextView android:layout_height="wrap_content" android:id="@+id/filelist" android:layout_width="fill_parent"></TextView>
</ScrollView><ExpandableListView android:id="@+id/ExpandableListView01" android:layout_height="wrap_content" android:layout_width="fill_parent"></ExpandableListView>

<TableRow android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/destLine"><TextView android:layout_width="wrap_content" android:id="@+id/labelDest" android:layout_height="wrap_content" android:text="New Album" android:layout_margin="5px"></TextView><EditText android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/destination" android:text="New"></EditText>
</TableRow><TableRow android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/recentLine"><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_gravity="center" android:id="@+id/labelRecent" android:text="Or" android:layout_margin="5px"></TextView><Spinner android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/recent"></Spinner>

</TableRow><LinearLayout android:layout_height="wrap_content" android:layout_width="fill_parent" android:id="@+id/buttonLine"><Button android:layout_height="wrap_content" android:text="Cancel" android:id="@+id/cancel" android:layout_width="wrap_content"></Button><Button android:layout_height="wrap_content" android:id="@+id/ok" android:text="Move" android:layout_width="fill_parent"></Button>
</LinearLayout>

</LinearLayout>
